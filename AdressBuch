public class AdressBuch {
	
	private String vorName;
	private String nachName;
	private String adresse;
	private int postLeitZahl;
	private int hausNummer;
	private String adressZusatzt;
	private String stadt;
	private int telefonnummer;
	private String emailAdresse;
	
	// Getter und Setter der Attribute definieren
	
	public String getVorName() {
		return vorName;
	}
	public void setVorName(String vorName) {
		this.vorName = vorName;
	}
	public String getAdresse() {
		return adresse;
	}
	public void setAdresse(String adresse) {
		this.adresse = adresse;
	}
	public String getNachName() {
		return nachName;
	}
	public void setNachName(String nachName) {
		this.nachName = nachName;
	}
	public int getPostLeitZahl() {
		return postLeitZahl;
	}
	public void setPostLeitZahl(int postLeitZahl) {
		this.postLeitZahl = postLeitZahl;
	}
	public String getStadt() {
		return stadt;
	}
	public void setStadt(String stadt) {
		this.stadt = stadt;
	}
	public int getHausNummer() {
		return hausNummer;
	}
	public void setHausNummer(int hausNummer) {
		this.hausNummer = hausNummer;
	}
	public int getTelefonnummer() {
		return telefonnummer;
	}
	public void setTelefonnummer(int telefonnummer) {
		this.telefonnummer = telefonnummer;
	}
	public String getEmailAdresse() {
		return emailAdresse;
	}
	public void setEmailAdresse(String emailAdresse) {
		this.emailAdresse = emailAdresse;
	}
	public String getAdressZusatzt() {
		return adressZusatzt;
	}
	public void setAdressZusatzt(String adressZusatzt) {
		this.adressZusatzt = adressZusatzt;
	}
}
